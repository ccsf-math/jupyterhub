{
    "@jupyter-server/resource-usage:topbar-item": {
        "enable": true,
        "refreshRate": 5000,
        "memory": {
            "label": "Mem: "
        },
        "cpu": {
            "label": "CPU: "
        }
    },
    "@jupyterlab/apputils-extension:notification": {
        "fetchNews": "false",
        "checkForUpdates": false,
        "doNotDisturbMode": false
    }, 
    "@jupyterlab/mainmenu-extension:plugin": {
        "menus": [
            {
                "disabled": false,
                "id": "jp-mainmenu-file",
                "items": [
                    // Disable deafult Download item
                    {
                        "disabled": true,
                        "command": "docmanager:download",
                        "rank": 6,
                    },
                    // Add Download HTML item
                    {
                        "command": "notebook:export-to-format", 
                        "rank": 6,
                        "args": {
                            "format": "html",
                            "label": "Download HTML"
                        }
                    },
                    // Add Download IPYNB item
                    {
                        "command": "notebook:export-to-format", 
                        "rank": 7,
                        "args": {
                            "format": "ipynb",
                            "label": "Download IPYNB"
                        }
                    },
                    // Disable default Save and Export Notebook AS submenu item
                    {
                        "disabled": true,
                        "type": "submenu",
                        "rank": 10,
                        "submenu": {
                            "id": "jp-mainmenu-file-notebookexport",
                            "label": "Save and Export Notebook As"
                        }
                    }
                ]
            },
            {
                "disabled": false,
                "id": "jp-mainmenu-git",
                "items": [
                    {
                        "command": "git:init"
                    },
                    {
                        "command": "git:clone"
                    },
                    {
                        "command": "git:merge"
                    },
                    {
                        "command": "git:push"
                    },
                    {
                        "command": "git:push",
                        "args": {
                            "advanced": true
                        }
                    },
                    {
                        "command": "git:pull"
                    },
                    {
                        "command": "git:pull",
                        "args": {
                            "force": true
                        }
                    },
                    {
                        "command": "git:reset-to-remote"
                    },
                    {
                        "command": "git:manage-remote"
                    },
                    {
                        "command": "git:terminal-command"
                    },
                    {
                        "type": "separator"
                    },
                    {
                        "command": "git:toggle-simple-staging"
                    },
                    {
                        "command": "git:toggle-double-click-diff"
                    },
                    {
                        "type": "separator"
                    },
                    {
                        "command": "git:open-gitignore"
                    },
                    {
                        "type": "separator"
                    },
                    {
                        "type": "submenu",
                        "submenu": {
                            "id": "jp-mainmenu-git-help",
                            "label": "Help",
                            "items": [
                                {
                                    "command": "git:open-url",
                                    "args": {
                                        "text": "Set Up Remotes",
                                        "url": "https://www.atlassian.com/git/tutorials/setting-up-a-repository"
                                    }
                                },
                                {
                                    "command": "git:open-url",
                                    "args": {
                                        "text": "Git Documentation",
                                        "url": "https://git-scm.com/doc"
                                    }
                                }
                            ]
                        }
                    }
                    ]
            }
        ]
    }
}
