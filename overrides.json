{
    "@jupyter-server/resource-usage:topbar-item": {
        "enable": true,
        "refreshRate": 5000,
        "memory": {
            "label": "Mem: "
        },
        "cpu": {
            "label": "CPU: "
        }
    },
    "@jupyterlab/apputils-extension:notification": {
        "checkForUpdates": false,
        "doNotDisturbMode": true,
        "fetchNews": "false"
    }, 
    "@jupyterlab/mainmenu-extension:plugin": {
        "menus": [
            {
                "disabled": false,
                "id": "jp-mainmenu-file",
                "items": [
                    {
                        "command": "docmanager:download",
                        "rank": 6,
                        "disabled": true
                    },
                    {
                        "type": "submenu",
                        "disabled": true,
                        "rank": 10,
                        "submenu": {
                            "id": "jp-mainmenu-file-notebookexport",
                            "label": "Save and Export Notebook As"
                        }
                    }
                ],
                "label": "File",
                "mnemonic": -1,
                "rank": 1
            },
            {
                "name": "Download HTML",
                "command": "notebook:export-to-format", 
                "rank": 6,
                "args": {
                    "format": "html",
                    "label": "Download HTML"
                }
            },
            {
                "name": "Download IPYNB", 
                "command": "notebook:export-to-format", 
                "rank": 7,
                "args": {
                    "format": "ipynb",
                    "label": "Download IPYNB"
                }
            },
            {
                "disabled": true,
                "id": "jp-mainmenu-git",
                "label": "Git",
                "mnemonic": -1,
                "rank": 60
            }
        ]
    }
}
